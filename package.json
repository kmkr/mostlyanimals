{
  "name": "splendid-nudibranch",
  "version": "1.0.0",
  "main": "server/index.js",
  "scripts": {
    "start": "NODE_ENV=production && npm run package && npm run start:server",
    "start:server": "babel-node --presets es2015 server/index.js",
    "start:client": "webpack --watch --config client/webpack.config.js",
    "build": "npm run lint && npm run test",
    "package": "webpack -p --config client/webpack.config.js",
    "lint": "eslint .",
    "test": "npm run test:client ; npm run test:server",
    "test:client": "karma start client/karma.conf.js --single-run",
    "test:client:watch": "karma start client/karma.conf.js",
    "test:server": "mocha -R spec --compilers js:babel-register --recursive \"server/**/*_test.js\"",
    "test:server:watch": "mocha -w -R spec --compilers js:babel-register --recursive \"server/**/*_test.js\""
  },
  "dependencies": {
    "aws-sdk": "^2.2.43",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.2",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "body-parser": "^1.15.0",
    "compression": "^1.6.1",
    "core-js": "^2.1.5",
    "css-loader": "^0.23.1",
    "es6-promise": "^3.1.2",
    "eslint": "^2.2.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^4.2.1",
    "exports-loader": "^0.6.3",
    "express": "^4.13.4",
    "gm": "^1.21.1",
    "mongodb": "^2.1.7",
    "morgan": "^1.7.0",
    "multer": "^1.1.0",
    "node-sass": "^3.4.2",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.0",
    "redux": "^3.3.1",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.0.1",
    "sass-loader": "^3.2.0",
    "sitemap": "^1.5.0",
    "smooth-scroll": "github:cferdinandi/smooth-scroll",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.14",
    "whatwg-fetch": "^0.11.0"
  },
  "devDependencies": {
    "babel-register": "^6.4.3",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.2.0",
    "chai-enzyme": "^0.4.1",
    "enzyme": "^2.1.0",
    "imports-loader": "^0.6.5",
    "json-loader": "^0.5.4",
    "karma": "^0.13.19",
    "karma-mocha": "^0.2.1",
    "karma-mocha-reporter": "^2.0.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.4.5",
    "phantomjs-prebuilt": "^2.1.3",
    "proxyquire": "^1.7.3",
    "react-addons-test-utils": "^0.14.7",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0"
  }
}
