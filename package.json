{
  "name": "splendid-nudibranch",
  "version": "1.0.0",
  "main": "server/index.js",
  "engines": {
    "node": "6.4.0"
  },
  "scripts": {
    "clean": "rimraf lib/",
    "start": "NODE_ENV=production && npm run clean && npm run package:client && npm run start:server",
    "start:server": "npm run package:server && node lib/server/index.js",
    "start:client": "webpack --watch --config client/webpack.config.js",
    "build": "npm run lint && npm run test",
    "package:client": "webpack -p --config client/webpack.config.js",
    "package:server": "babel server --out-dir lib/server && babel common --out-dir lib/common && ncp ./server/static/images ./lib/server/static/images && node build-scripts/move-layouts.js",
    "lint": "eslint .",
    "test": "npm run test:client ; npm run test:server",
    "test:client": "karma start client/karma.conf.js --single-run",
    "test:client:watch": "karma start client/karma.conf.js",
    "test:server": "mocha -R spec --compilers js:babel-register --recursive \"server/**/*_test.js\"",
    "test:server:watch": "mocha -w -R spec --compilers js:babel-register --recursive \"server/**/*_test.js\""
  },
  "dependencies": {
    "autoprefixer": "^6.4.0",
    "aws-sdk": "^2.5.2",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "core-js": "^2.4.1",
    "css-loader": "^0.24.0",
    "debounce": "^1.0.0",
    "es6-promise": "^3.2.1",
    "eslint": "^3.3.1",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-react": "^6.1.2",
    "exports-loader": "^0.6.3",
    "express": "^4.14.0",
    "express-react-views": "^0.10.2",
    "gm": "^1.23.0",
    "mongodb": "^2.2.7",
    "morgan": "^1.7.0",
    "multer": "^1.2.0",
    "ncp": "^2.0.0",
    "node-sass": "^3.8.0",
    "object.entries": "^1.0.3",
    "postcss-loader": "^0.11.1",
    "precss": "^1.4.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "sass-loader": "^4.0.0",
    "sitemap": "^1.8.1",
    "smooth-scroll": "github:cferdinandi/smooth-scroll",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "whatwg-fetch": "^1.0.0",
    "why-did-you-update": "0.0.8"
  },
  "devDependencies": {
    "babel-register": "^6.11.6",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "chai-enzyme": "^0.5.0",
    "enzyme": "^2.4.1",
    "imports-loader": "^0.6.5",
    "json-loader": "^0.5.4",
    "karma": "^1.2.0",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.1.0",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "mocha": "^3.0.2",
    "phantomjs-prebuilt": "^2.1.12",
    "proxyquire": "^1.7.10",
    "react-addons-test-utils": "^15.3.1",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0"
  }
}
