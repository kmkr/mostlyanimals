{
  "name": "splendid-nudibranch",
  "version": "1.0.0",
  "main": "server/index.js",
  "engines": {
    "node": ">=6.4.0"
  },
  "scripts": {
    "start": "NODE_ENV=production node lib/server/index.js",
    "start:server": "npm run package:server && node lib/server/index.js",
    "start:client": "webpack --watch --config client/webpack.config.js",
    "build": "npm run lint && npm run test && npm run package",
    "prepackage": "rimraf lib/",
    "package": "npm run package:client && npm run package:server",
    "package:client": "NODE_ENV=production webpack -p --config client/webpack.config.js",
    "package:server": "babel server --out-dir lib/server && babel common --out-dir lib/common && ncp ./server/static/images ./lib/server/static/images && node build-scripts/move-layouts.js",
    "lint": "eslint .",
    "test": "jest",
    "test:watch": "npm run test -- --watch"
  },
  "dependencies": {
    "aws-sdk": "^2.7.7",
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "core-js": "^2.4.1",
    "es6-promise": "^4.0.5",
    "express": "^4.14.0",
    "express-react-views": "^0.10.2",
    "gm": "^1.23.0",
    "mongodb": "^2.2.11",
    "morgan": "^1.7.0",
    "multer": "^1.2.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.1.0",
    "sitemap": "^1.8.2",
    "whatwg-fetch": "^2.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.16.0",
    "babel-plugin-transform-object-rest-spread": "^6.19.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.18.0",
    "css-loader": "^0.26.0",
    "eslint": "^3.11.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-react": "^6.7.1",
    "exports-loader": "^0.6.3",
    "jest": "^19.0.0",
    "ncp": "^2.0.0",
    "postcss-loader": "^1.1.1",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(scss)$": "<rootDir>/__mocks__/style-mock.js"
    },
    "testRegex": "<rootDir>/(client|server|common)/.*_test\\.js"
  }
}
